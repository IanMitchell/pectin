{
  "command": {
    "publish": {
      "allowBranch": "latest",
      "conventionalCommits": true,
      "message": "chore(release): %s"
    }
  },
  "ignoreChanges": [
    "**/__tests__/**",
    "**/test/**",
    "*.test.js",
    "*.md"
  ],
  "packages": [
    "packages/*"
  ],
  "version": "2.4.0"
}
