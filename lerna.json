{
  "command": {
    "publish": {
      "allowBranch": "1.x",
      "conventionalCommits": true,
      "message": "chore(release): %s",
      "npmTag": "legacy"
    }
  },
  "ignoreChanges": [
    "**/__tests__/**",
    "**/test/**",
    "*.test.js",
    "*.md"
  ],
  "packages": [
    "packages/*"
  ],
  "version": "1.3.0"
}
